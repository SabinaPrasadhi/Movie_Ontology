###  http://www.semanticweb.org/prasadhiranasinghe/ontologies/2020/8/untitled-ontology-7#ActionComedyMovie

:ActionComedyMovie rdf:type owl:Class ;
                   
                   rdfs:subClassOf :CrossGenreMovie ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasGenre ;
                                     owl:allValuesFrom [ rdf:type owl:Class ;
                                                         owl:unionOf ( :ActionGenre
                                                                       :ComedyGenre
                                                                     )
                                                       ]
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasGenre ;
                                     owl:someValuesFrom :ComedyGenre
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasGenre ;
                                     owl:someValuesFrom :ActionGenre
                                   ] ;
                   
                   rdfs:comment "A movie that only has action and comedy genres" .



