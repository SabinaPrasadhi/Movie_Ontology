:EmotionalMovie rdf:type owl:Class ;
                
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:intersectionOf ( :Movie
                                                           [ rdf:type owl:Class ;
                                                             owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :hasGenre ;
                                                                             owl:someValuesFrom :DisasterThrillerGenre
                                                                           ]
                                                                           [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :hasGenre ;
                                                                             owl:someValuesFrom :RomanceGenre
                                                                           ]
                                                                         )
                                                           ]
                                                         )
                                    ] ;
                
                rdfs:comment "A movie which belongs to either disaster thriller genre or romantic genre" .



